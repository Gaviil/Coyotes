    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    <style media="screen">
    body{
          background: #222;
    }
    .circle{
       position:absolute;
      width:100%;
      left:0;
