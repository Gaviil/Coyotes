    .circle img{
      width:120px;
      height:120px;
      overflow:hidden;
      border-radius:99em;
      display:table;
      margin:0 auto;
      margin-top:50px;
    }

    </style>
</head>
<body>

        <%= yield %>

</body>
</html>
